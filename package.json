{
  "name": "tinyclaw",
  "version": "0.0.2",
  "description": "TinyClaw is a tiny agent that acts as your 24/7 personal assistant",
  "scripts": {
    "deploy": "cd ~/workspace/tinyclaw && git pull && npm run restart",
    "restart": "npm run build && npm run launchd:install && ~/.local/bin/tinyclaw restart",
    "build": "tsc && tsc -p tsconfig.visualizer.json && echo '{\"type\":\"module\"}' > dist/visualizer/package.json",
    "build:main": "tsc",
    "build:visualizer": "tsc -p tsconfig.visualizer.json && echo '{\"type\":\"module\"}' > dist/visualizer/package.json",
    "discord": "node dist/channels/discord-client.js",
    "queue": "node dist/queue-processor.js",
    "visualize": "node dist/visualizer/team-visualizer.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "launchd:install": "bash scripts/launchd.sh install",
    "launchd:uninstall": "bash scripts/launchd.sh uninstall"
  },
  "dependencies": {
    "@types/react": "^19.2.14",
    "discord.js": "^14.16.0",
    "dotenv": "^16.4.0",
    "ink": "^6.7.0",
    "ink-gradient": "^4.0.0",
    "ink-spinner": "^5.0.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
